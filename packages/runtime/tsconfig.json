{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@nx.js/ws/frame": ["../ws/src/frame.ts"]
    },
    "noEmit": true,
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true
  },
  "include": ["src/**/*.ts"]
}
